    <item>
      <title><![CDATA[Social Newletter]]></title>
      <identifier><![CDATA[block-footer-top]]></identifier>
      <content><![CDATA[<div class="container">
	<div class="main">
		<div class="footer">
			<div class="row">
				<div class="col-md-3 col-sm-6 col-xs-12">
					<h2 class="logo-text">milano fashion.</h2>
				</div>
				<div class="col-md-3 col-sm-6 col-xs-12">
					<ul class="list-inline">
						<li><a class="social-link twitter" href="#"><em class="fa fa-twitter"><span class="hidden">twitter</span></em></a></li>
						<li><a class="social-link google-plus" href="#"><em class="fa fa-google-plus"><span class="hidden">google-plus</span></em></a></li>
						<li><a class="social-link facebook" href="#"><em class="fa fa-facebook"><span class="hidden">facebook</span></em></a></li>
						<li><a class="social-link tumblr" href="#"><em class="fa fa-tumblr"><span class="hidden">tumblr</span></em></a></li>
						<li><a class="social-link pinterest" href="#"><em class="fa fa-pinterest"><span class="hidden">pinterest</span></em></a></li>
					</ul>
				</div>
				<div class="col-md-6 col-sm-12 col-xs-12 clearboth">
					<div class="collapsible mobile-collapsible">
						<div class="block-newletter">{{block class="Magento\Newsletter\Block\Subscribe" template="subscribe.phtml"}}</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>]]></content>
      <is_active><![CDATA[1]]></is_active>
